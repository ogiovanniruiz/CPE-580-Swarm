<launch>
		<!-- joy node -->
  	<node respawn="true" pkg="joy"
        	type="joy_node" name="joy_stick" >
    	<param name="dev" type="string" value="/dev/input/js0" />
    	<param name="deadzone" value="0.12" />
  	</node>

	<!-- control node-->
	<node name="control_node" pkg="swarm" type="control.py" output="screen" />	


	<!-- simulation node-->
	<node name="sim_node" pkg="swarm" type="sim.py" output="screen" />

</launch>
	
